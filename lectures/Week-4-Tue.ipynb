    "%matplotlib inline\n",
    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "import scipy.stats as stats\n",
    "dist = stats.beta\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(12, 12))\n",
    "\n",
    "# define the number of trials\n",
    "n_trials = [0, 1, 2, 3, 4, 5, 8, 15, 50, 500]\n",
    "pTrue=0.7\n",
    "#data = stats.bernoulli.rvs(pTrue, size=n_trials[-1])\n",
    "\n",
    "\n",
    "#define the sampling of the parameter space\n",
    "x = np.linspace(0, 1, 100)\n",
    "\n",
    "#define the probability/likelihood for N observations and the prior on theta\n",
    "def likelihood(h, n, theta):\n",
    "    '''Likelihood of h heads given n observations and probability of a head theta'''\n",
    "    return theta**h * (1-theta)**(n-h)\n",
    "\n",
    "def prior(theta, loc =0.3, scale=0.3):\n",
    "    '''Return a Gaussian prior on theta'''\n",
    "    return stats.norm.pdf(theta, loc, scale)\n",
    "\n",
    "\n",
    "# step through the sample taking subsets\n",
    "for k, N in enumerate(n_trials):\n",
    "    sx = plt.subplot(len(n_trials)/2, 2, k+1)\n",
    "    plt.xlabel(\"$p$, probability of heads\") \\\n",
    "        if k in [0, len(n_trials)-1] else None\n",
    "    plt.setp(sx.get_yticklabels(), visible=False)\n",
    "    heads = data[:N].sum()\n",
    "    \n",
    "    #calculate posterior\n",
    "    posterior = likelihood(heads, N, x)*prior(x)\n",
    "    posterior /= posterior.sum()\n",
    "\n",
    "    plt.plot(x, posterior, label=\"%d tosses,\\n %d heads\" % (N, heads))\n",
    "    \n",
    "    plt.axvline(x=pTrue, color=\"k\", linestyle=\"--\", lw=1)\n",
    "    plt.axvline(x=heads/N, color=\"red\", linestyle=\"--\", lw=1)\n",
    "\n",
    "\n",
    "    leg = plt.legend()\n",
    "    leg.get_frame().set_alpha(0.4)\n",
    "\n",
    "\n",
    "\n",
    "plt.suptitle(\"Bayesian updating of posterior probabilities\",\n",
    "             y=1.02,\n",
    "             fontsize=14)\n",
    "\n",
    "plt.tight_layout()"
